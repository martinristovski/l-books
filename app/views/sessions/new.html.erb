<div class = "signin_prompt">
    <h1 >Sign In</h1>
    <br/>
    <%= form_with url: signin_path do |f| %>
        <p>
            <%= f.label 'email:'%><br>
            <%= f.text_field :email, id: 'email' %>
        </p>
        <p >
            <%= f.label 'password:'%><br>
            <%= f.password_field :password, id: 'password' %>
        </p>
        <%= f.hidden_field :redirect_url, value: @fdata[:redirect_url] %>
        <br/>
        <p>
            <%= f.submit 'Log In' %>
        </p>
    <% end %>
</div>